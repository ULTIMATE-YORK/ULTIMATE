<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<VBox xmlns:fx="http://javafx.com/fxml" spacing="10" alignment="TOP_LEFT" prefWidth="600"
	prefHeight="600" fx:controller="controllers.VerificationRunBoxController">

	<padding>
		<Insets top="10" right="10" bottom="10" left="10" />
	</padding>

	<Label text="Run Details" />
	<ListView fx:id="details" />

	<Label text="Results" />
	<ListView fx:id="results" />

	<HBox alignment="CENTER" spacing="10.0">
		<Button fx:id="plot" onAction="#plot" text="Plot" />
		<Button fx:id="export" onAction="#export" text="Export" />
		<Button fx:id="closeButton" text="Close" onAction="#close" />
	</HBox>

</VBox>